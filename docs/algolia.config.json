{
  "index_name": "pinia_orm_docs",
  "start_urls": [
    "https://pinia-orm.codedredd.de/"
  ],
  "stop_urls": [],
  "selectors": {
    "lvl0": {
      "selector": "",
      "default_value": "Documentation"
    },
    "lvl1": {
      "selector": "article h1"
    },
    "lvl2": "article h2",
    "lvl3": "article h3",
    "lvl4": "article h4",
    "lvl5": "article h5",
    "content": "article p, article li",
    "language": {
      "selector": ".exists-probably .language",
      "global": true,
      "default_value": ["en"]
    }
  },
  "custom_settings": {
    "attributesForFaceting": [
      "language"
    ],
    "attributesToRetrieve": [
      "hierarchy", "content", "anchor", "url"
    ],
    "attributesToHighlight": ["hierarchy", "content"],
    "attributesToSnippet": ["content:10"],
    "distinct": true
  }
}
